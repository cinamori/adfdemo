{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cigithubdemo"
		},
		"ciadlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ciadlsgen2'"
		},
		"ciadlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cidatalakegen2.dfs.core.windows.net/"
		},
		"AzureDataLakeStoragePE_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/5b0954ff-3dce-4d45-842a-a27063126514/resourceGroups/CICDDemo/providers/Microsoft.Storage/storageAccounts/cidevadlsgen2"
		},
		"AzureDataLakeStoragePE_properties_groupId": {
			"type": "string",
			"defaultValue": "dfs"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cigithubdemo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ciadlsgen2_accountKey": {
						"value": "[parameters('ciadlsgen2_accountKey')]"
					},
					"ciadlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ciadlsgen2_properties_typeProperties_url')]"
					},
					"AzureDataLakeStoragePE_properties_privateLinkResourceId": {
						"value": "[parameters('AzureDataLakeStoragePE_properties_privateLinkResourceId')]"
					},
					"AzureDataLakeStoragePE_properties_groupId": {
						"value": "[parameters('AzureDataLakeStoragePE_properties_groupId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}